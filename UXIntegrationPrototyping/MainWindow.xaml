<Window x:Class="UXIntegrationPrototyping.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBox Width="420" Height="20" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0, 0, 0, 10"></TextBox>
        <StackPanel HorizontalAlignment="Center" Width="420" VerticalAlignment="Center" Height="100" Orientation="Horizontal">
            <Button Name="MyButton" Content="MyButton" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0, 0, 10, 0" Width="200" Height="100"/>
            <Button Name="YourButton" Content="YourButton" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10, 0, 0, 0" Width="200" Height="100"/>
        </StackPanel>
        </StackPanel>
    </Grid>
</Window>
